                          {{ custom_icon_second }}
                          <span class="h5 m-zero">{{- block.settings.secondary_heading | escape -}}</span>
                        </div>
                      {%- endif -%}
                      <div class="hide-empty text-block">{{- block.settings.secondary_content -}}</div>
                    </div>
                    <div class="clm" style="--w-xs: 12; --w-sm: 6;">
                      {% if block.settings.tertiary_heading != blank %}
                        <div class="info-card__heading mb-12 d-grid gap-16 gap-md-24">
                          {{ custom_icon_third }}
                          <span class="h5 m-zero">{{- block.settings.tertiary_heading | escape -}}</span>
                        </div>
                      {% endif %}
                      <div class="hide-empty text-block">{{- block.settings.tertiary_content -}}</div>
                    </div>
                    <div class="clm" style="--w-xs: 12; --w-sm: 6;">
                      {% if block.settings.quaternary_heading != blank %}
                        <div class="info-card__heading mb-12 d-grid gap-16 gap-md-24">
                          {{ custom_icon_fourth }}
                          <span class="h5 m-zero">{{- block.settings.quaternary_heading | escape -}}</span>
