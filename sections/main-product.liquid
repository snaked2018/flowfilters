            {%- when 'pickup_availability' -%}
              {%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities | where: 'pick_up_enabled', true -%}
              {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %}
                <div class="product__info ord-{{ forloop.index }} {{ block.settings.show_expected_pickup_time | default: '' | replace: 'true', 'pickup_availability--show-pickup-time' }}" {{ block.shopify_attributes }}>
                  <sht-pickup-avail
                    class="pickup-availability corner-radius-sm color-accent d-block no-js-hidden pb-40 pl-24 pr-24 pt-32 p-relative"
                    {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %}
                      available
                    {% endif %}
                    data-base-url="{{ shop.url }}{{ routes.root_url }}"
                    data-variant-id="{{ product.selected_or_first_available_variant.id }}"
                    data-has-only-default-variant="{{ product.has_only_default_variant }}"
                    data-show-expected-pickup-time="{{ block.settings.show_expected_pickup_time }}"
                  >
                    <template>
                      <sht-pickup-avail-prwv class="d-flex top-xs pickup-availability__preview">
                        {% render 'icon-check-mark' %}
                        <div class="pickup-availability__info">
                          <p>{{ 'products.product.pickup_availability.unavailable' | t }}</p>
                          <button class="btn btn-small btn-plain">{{ 'products.product.pickup_availability.refresh' | t }}</button>
                        </div>
                      </sht-pickup-avail-prwv>
                    </template>
                  </sht-pickup-avail>
                </div>
              {% endif %}
            {%- when 'product_cross_sell' -%}
              <div class="product__info ord-{{ forloop.index }}" {{ block.shopify_attributes }}>
                <h2 class="d-block h5  mb-24 mb-md-40">{{ block.settings.heading }}</h2>
                <div class="cross-sell__items gap-unequal rw">
                  {%- for cross_sell in block.settings.product_list -%}
                    <div class="clm" style="--w-xs: 6;--w-sm: 6;--w-md: 6;">
                      {% render 'card-product', product: cross_sell, show_rating: false, show_vendor: true, show_color_swatch: false, show_sale_badge: true, enable_quick_buy: false, item_per_row: 2, ratio: product_card_media, section: section %}
                    </div>
                  {%- endfor -%}
                </div>
              </div>
            {%- when 'custom_text' -%}
              <div class="product__info ord-{{ forloop.index }}  hide-empty {{ block.settings.text_style | replace: 'color-light', 'color-light' | replace: 'body', 'color-text-regular' }} " {{ block.shopify_attributes }}>{{- block.settings.text -}}</div>
            {%- when 'custom_liquid' -%}
              <div class="product__info ord-{{ forloop.index }}  hide-empty" {{ block.shopify_attributes }}>{{- block.settings.liquid_code -}}</div>
            {%- when 'popup_content' -%}
              {%- liquid
                assign popup_content_dialog_id = block.id | append: section.id | md5 | truncate: 12, ""
                assign popup_content_block = block
                assign size_popup_content_cta = 'disabled'
                if block.settings.static_page != blank
                  assign size_popup_content_cta = ''
                endif
              %}
              <div class="product__info ord-{{ forloop.index }} " {{ block.shopify_attributes }}>
                <button class="btn btn-link fs-body" type="button" aria-haspopup="dialog" onclick="window.dialogPopupContent{{ popup_content_dialog_id }}.showModal(this, true)" {{ size_popup_content_cta }}>{{- block.settings.text -}}</button>
              </div>
            {%- when 'complementary_products' -%}
              <sht-prd-recom class="complementary-products product__info" data-url="{{ routes.product_recommendations_url }}?section_id={{ section.id }}&product_id={{ product.id }}&limit={{ block.settings.products_to_show }}&intent=complementary">
                {% if recommendations.products_count > 0 %}
                  <h2 class="h5 hide-empty mb-48 d-block"> {{- block.settings.heading -}} </h2>
                {% endif %}
                <div class="rw gap-unequal">
                  {% for recommendation in recommendations.products limit: block.settings.products_to_show %}
                     <div class="clm" style="--w-xs: 6">
                      {% render 'card-product',
                        product: recommendation,
                        ratio: settings.product_card_image_ratio,
                        show_second_image: settings.show_second_image,
                        show_vendor: settings.show_product_vendor,
                        show_rating: settings.show_product_rating,
                        show_color_swatch: settings.show_color_swatches,
                        show_sale_badge: settings.show_sale_badge,
                        item_per_row: section.settings.item_per_row,
                        enable_quick_buy: settings.enable_quick_buy,
                        section: section
                     %}
                     </div>
                   {% endfor %}
                </div>
              </sht-prd-recom>
          {%- endcase -%}
        {%- endfor -%}
      </div>
    </div>
  </div>
  {%- render 'section-id', section: section -%}
</div>
{%- liquid
  if popup_content_block
    render 'product-popup-content', popup_content_dialog_id: popup_content_dialog_id, popup_content_block: popup_content_block
  endif
  render 'main-product-gallery-dialog', product: product, place_holder: place_holder, rand: section_id, ratio: ratio_thubnails, object_fit: object_fit
-%}
{%- if is_app -%}
