                            {{ custom_icon_fourth }}
                            <span class="h6 m-zero fg-1 ml-8 ta-left">{{ block.settings.quaternary_heading | escape }}</span>
                            <span class="accordion__toggle zi-1 d-flex">{{ accordion_icons }}</span>
