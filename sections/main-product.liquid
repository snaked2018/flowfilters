{%- liquid
  assign variant_label = 'color,colour,couleur,colore,boja,farbe,색,色,warna,dath,боја,lliw,liw,колір,värvi,värv,գույն,krāsa,màu sắc,màu,cor,cцвят,färg,farve,ngjyrë,kleur,väri,farge,renk,culoare,szín,цвет,kolor,barva,χρώμα,litur,spalva,farba' | split: ','
  assign place_holder = 'product-1' | placeholder_svg_tag: 'placeholder js-image-lazy w-100 h-100 p-absolute'
  assign section_id = section.id | md5 | truncate: 12, ""
  assign is_first_image = false
  assign is_app = false
  assign product_form_id = 'productForm-' | append: section.id

  assign product_form_installment_id = 'productFormInstallment-' | append: section.id
  assign first_3d_model = product.media | where: 'media_type', 'model' | first
  assign popup_content_block = nil
  assign popup_content_dialog_id = ''

  assign enable_thumb_mobile = 'd-none d-flex-md'
  if section.settings.enable_thumb_mobile
    assign enable_thumb_mobile = 'd-flex top-xs'
  endif
  assign product_card_media = settings.product_card_image_ratio
  assign thumb_index = 0

  assign ratio_thubnails = section.settings.thumbnails_ratio
  assign object_fit = 'of-cover'
  if ratio_thubnails contains 'fit'
    assign ratio_thubnails = ratio_thubnails | replace: 'fit', ''
    assign object_fit = 'of-contain'
  endif
%}
{%- if first_3d_model -%}
  <script type="application/json" id="ProductJSON-{{ product.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{%- endif -%}
<div class="main-product ctnr {{ section.settings.color_scheme }}" id="{{ section.id }}" data-ui-component="Main Product Section">
  <div class="product-details background d-grid c-gap-md top-xs  gap-36 p-relative zi-1" style="--gtc-equal:1; --gtc-equal-md:2">
    <div class="product__media-wrapper w-100 o-hidden js-animate anm anm-fadeInUpLarge anm-main-product {% if section.settings.enable_sticky %} product__media--sticky {% endif %}" style="--duration: .7s;" role="region" aria-label="{{- 'accessibility.view_gallery' | t -}}">
      <a class="visually-hidden skip-to-content btn btn-primary d-block ta-center" href="#productInfo-{{ section.id }}">{{ 'accessibility.skip_to_product_information' | t }}</a>
      <div class="product__media-wrapper--single-with-thumbnails ">
          <sht-prd-slideshow role="status" class="product-slideshow d-flex fd-column gap-24 d-grid-md p-relative" data-hide-variant-image="true" data-section="{{ section.id }}">
            <div class="product-slideshow__thumbnail zi-2 no-js-hidden {{ enable_thumb_mobile }}">
              <sht-vert-carousel class="carousel--vertical w-100" data-section="{{ section.id }}">
                <div class="product-slideshow__thumbs {{ enable_thumb_mobile }} h-100 top-xs js-carousel-items vertical-carousel vertical-carousel__container fd-column-md p-zero m-zero oy-auto p-relative disable-scrollbars fw-nowrap scrollable scrollable-y scrl-bhvr-auto" role="list" aria-label="{{ 'accessibility.slider' | t }}">
                  {%- liquid
                    if product.selected_or_first_available_variant.featured_media != null
                      assign thumb_index = thumb_index | plus: 1
                      assign featured_media = product.selected_or_first_available_variant.featured_media
                      render 'product-slideshow-thumb', section: section, media: featured_media, product: product, place_holder: place_holder, thumb_index: thumb_index, ratio: ratio_thubnails, object_fit: object_fit
                    endif
                    for media in product.media
                      assign thumb_index = thumb_index | plus: 1
                      unless media.id == product.selected_or_first_available_variant.featured_media.id
                        render 'product-slideshow-thumb', section: section, media: media, product: product, place_holder: place_holder, thumb_index: thumb_index, ratio: ratio_thubnails, object_fit: object_fit
                      endunless
                    endfor
                  -%}
                </div>
                <div class="product-slideshow__pagination carousel-pagination carousel-controls--outside maw-100 center-xs no-js-hidden js-carousel-pagination">
                  <div class="carousel-btn carousel-btn-prev p-absolute">
                    <button type="button" data-step="1" class="btn btn-secondary btn-small btn-icon icon btn-prev p-absolute js-carousel-prev-btn" disabled aria-label="{{ 'accessibility.previous_slide' | t }}" name="previous">{% render 'icon-arrow-left' %}</button>
                  </div>
                  <div class="carousel-btn carousel-btn-next p-absolute">
                  <button type="button" data-step="1" class="btn btn-secondary btn-small btn-icon icon btn-next p-absolute js-carousel-next-btn" disabled aria-label="{{ 'accessibility.next_slide' | t }}" name="next">{% render 'icon-arrow-right' %}</button>
                  </div>
                </div>
              </sht-vert-carousel>
            </div>
            <div class="product-slideshow__slider w-100 p-relative o-hidden">
              {% render 'product-gallery-carousel', product: product, section: section, place_holder: place_holder, rand: section_id, hide_variant_image: true, is_first_image: is_first_image, open_zoom_modal: section.settings.enable_media_zoom, ratio: section.settings.media_ratio %}
            </div>
          </sht-prd-slideshow>
        </div>
    </div>
    <div class="product__info-wrapper js-animate anm anm-fadeInUpLarge anm-main-product" style="--duration: .7s;">
      <div id="productInfo-{{ section.id }}" class="main-product-info-container product__info-container d-flex fd-column" role="region" aria-label="{{- 'accessibility.view_product_info' | t -}}">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when '@app' -%}
              <div class="product__info ord-{{ forloop.index }}">
                {% liquid
                  assign is_app = true
                  render block
                %}
              </div>
            {%- when 'title' -%}
              <div class="product__info ord-{{ forloop.index }} product__info--spacing product__title " {{ block.shopify_attributes }}>
                <div class="d-flex middle-xs between-xs">
                  <h1 class="h3 m-zero mr-12 wb-break-word">{{ product.title | escape }}</h1>
                  {%- if block.settings.enable_share -%}
                    <sht-share-prd-btn class="zi-2 p-relative" data-product-url="{{ product.selected_variant.url | default: product.url | prepend: request.origin }}">
                      <div class="js-web-share-api-container" hidden>
                        <button class="btn btn-large ls-none btn-icon icon btn btn-secondary js-web-share-api-btn" aria-label="{{ 'accessibility.social_share' | t }}">{% render 'icon-share' %}</button>
                      </div>
                      <sht-dtl-exposal>
                        <details class="js-none-web-share-api-container p-relative zi-2" id="productShareDetails-{{ block.id }}-{{ section.id }}">
                          <summary aria-controls="productShare-{{ block.id }}-{{ section.id }}" aria-expanded="false" class="btn btn-large ls-none btn-icon icon btn btn-secondary js-none-web-share-api-btn">
                            {% render 'icon-share', pros: 'aria-hidden="true" focusable="false"' %}
                          </summary>
                          <div id="productShare-{{ block.id }}-{{ section.id }}" class="p-absolute social-share-list mt-8">
                            {% render 'product-social-share', product: product %}
                          </div>
                        </details>
                      </sht-dtl-exposal>
                    </sht-share-prd-btn>
                  {%- endif -%}
                </div>
              </div>
            {%- when 'vendor' -%}
              <div class="product__info ord-{{ forloop.index }} product__info--spacing product__vendor " {{ block.shopify_attributes }}>
                <div class="color-light subtext wb-break-word">
                  <span class="visually-hidden">{{ 'accessibility.vendor' | t }}</span>
                  <span>{{ product.vendor }}</span>
                </div>
              </div>
            {%- when 'rating' -%}
              <div class="product__info ord-{{ forloop.index }} product__info--spacing product__rating " role="status" {{ block.shopify_attributes }}>
                {% render 'product-rating', product: product %}
              </div>
            {%- when 'price' -%}
              <div class="product__info ord-{{ forloop.index }} product__prices d-grid gap-12" {{ block.shopify_attributes }}>
                <div class="d-flex fw-wrap gap-8" id="productPrice-{{ section.id }}" role="status">
                  {% render 'product-price', product: product, target: product.selected_or_first_available_variant, price_class: 'price--large', price_style: 'mr-8' %}
                  {% if product.selected_or_first_available_variant.compare_at_price > product.selected_or_first_available_variant.price and block.settings.show_sale_badge %}
                    {%- liquid
                      assign discount_cost = product.selected_or_first_available_variant.compare_at_price | minus: product.selected_or_first_available_variant.price
                      assign discount_percent = discount_cost | times: 100.0 | divided_by: product.selected_or_first_available_variant.compare_at_price | round: 2
                    -%}
                    <div class="product__badge">
                      <span class="badge badge--sale subtext d-inline-block p-relative fw-700">
                        <span class="p-relative zi-1 tt-up">
                          {{- 'products.product.on_sale' | t }}
                            {{ discount_percent }}%
                          <span class="d-none-important">
                          {% if settings.currency_code_enabled %}
                            {{- discount_cost | money_with_currency -}}
                          {% else %}
                            {{- discount_cost | money -}}
                          {% endif %}
                            ({{ discount_percent }}%)
                          </span>
                        </span>
                      </span>
                    </div>
                  {% endif %}
                </div>
                {%- if block.settings.show_tax_and_shipping_policy -%}
                  <div class="color-light hide-empty subtext text-block">
                    {%- if cart.taxes_included -%}
                      {{- 'products.product.include_taxes' | t -}}
                    {%- endif -%}
                    {%- if shop.shipping_policy.body != blank -%}
                      {{- 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url -}}
                    {%- endif -%}
                  </div>
                {% endif %}
                {%- form 'product', product, id: product_form_installment_id, class: 'installment installment-form caption-large' -%}
                  <div class="form__field d-none-important">
                    <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                  </div>
                  {{ form | payment_terms }}
                {%- endform -%}
              </div>
            {%- when 'SKU' -%}
                <div class="product__info product__sku product__info--spacing gap-24 d-flex fd-column hide-empty" id="sku-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                  {%- if product.selected_or_first_available_variant.sku != blank -%}
                    <span class="variant-sku color-light">SKU: {{ product.selected_or_first_available_variant.sku }}</span>
                  {%- endif -%}
                </div>
            {%- when 'description' -%}
              <div class="product__info ord-{{ forloop.index }} product__description wb-break-word  rte" {{ block.shopify_attributes }}>
                <span class="d-block mb-16 h5">{{ 'products.product.description' | t }}</span>
                {% if block.settings.enable_more_btn %}
                  <sht-read-more>
                    <div class="expandable-content p-relative o-hidden js-expandable-content " data-expanded="false">
                      <div class="rte">{{ product.description }}</div>
                    </div>
                    <div class="expandable-content__btn w-100 ta-left p-relative">
                      <button type="button" class="btn btn-link js-expandable-content-btn" data-view-more="{{ 'products.read_more.view_more' | t }}" data-view-less="{{ 'products.read_more.view_less' | t }}">
                        {{ 'products.read_more.view_more' | t }}
                      </button>
                    </div>
                  </sht-read-more>
                {% else %}
                  <div class="rte">{{ product.description }}</div>
                {% endif %}
              </div>
            {%- when 'variants' -%}
              <div class="product__info ord-{{ forloop.index }} product__variants gap-24 d-flex fd-column" {{ block.shopify_attributes }}>
                {%- unless product.has_only_default_variant -%}
                  <div class="" id="variantPicker-{{ section.id }}">
                    <sht-variant-radios class="product__variants--radios d-flex fd-column gap-24 no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                      {%- for option in product.options_with_values -%}
                        {% assign index = forloop.index | minus: 1 %}
                        <fieldset class="form__fields product-option js-variant-radio-container d-flex fw-wrap p-zero m-zero b-zero wb-break-word">
                          <legend class="p-zero m-zero product-option__name">
                            <span class="fw-700 mr-8">{{ option.name }}</span>
                            <span class="js-variant-radio-option-name hide-empty" ata-option-index="{{ index }}" data-option-name="{{ option.name }}">
                              {{ product.selected_or_first_available_variant.options[index] }}
                            </span>
                          </legend>
                          {%- assign variant_name = option.name | downcase -%}
                          {%- if variant_label contains variant_name and settings.enable_variant_swatches -%}
                            {% render 'main-product-color-swatch', product: product, option: option, section: section, product_form_id: product_form_id, index: index %}
                          {%- else -%}
                            <ul class="ls-none d-flex fw-wrap gap-12">
                              {%- for value in option.values -%}
                                <li class="form__field js-variant-option-value product-option__value {% if product.selected_or_first_available_variant.available == false and option.selected_value == value %}product-option--sold-out{% endif %}" data-value="{{ value | escape }}-{{ index }}" data-option-index="{{ index }}">
                                  <input type="radio" class="form__control input--variant js-variant-radio-item js-variant-item visually-hidden" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}" name="{{ option.name }}" value="{{ value | escape }}" data-variant-value="{{ value | escape }}-{{ index }}" form="{{ product_form_id }}" {% if option.selected_value == value %} checked {% endif %}>
                                  <label class="form__label product-option__label d-block js-variant-option-label" for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}">{{ value }}</label>
                                </li>
                              {%- endfor -%}
                            </ul>
                          {%- endif -%}
                        </fieldset>
                      {%- endfor -%}
                      <script type="application/json">{{ product.variants | json }}</script>
                    </sht-variant-radios>
                  </div>
                  <noscript>
                    <div class="form__fields {% if product.has_only_default_variant %} hidden{% endif %}">
                      <div class="form__field">
                        <label class="form__label" for="Variants-{{ section.id }}-{{ forloop.index0 }}">{{ 'products.product.product_variants' | t }}</label>
                        <select name="id" id="Variants-{{ section.id }}-{{ forloop.index0 }}" class="form__control w-100 h-100 field__text field--focus field--hover" form="{{ product_form_id }}">
                          {%- for variant in product.variants -%}
                            <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} {% if variant.available == false %} disabled {% endif %} value="{{ variant.id }}">
                              {{ variant.title }}
                              {%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}- {{ variant.price | money | strip_html }}
                            </option>
                          {%- endfor -%}
                        </select>
                      </div>
                    </div>
                  </noscript>
                {%- endunless -%}
                <div class="hide-empty fw-700" id="inventoryTracking-{{ section.id }}" role="status">
                  {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                    {%- liquid
                      assign stock_status = 'product--instock color-success'
                      if settings.product_information_low_stock_threshold > 0 and settings.product_information_low_stock_threshold >= product.selected_or_first_available_variant.inventory_quantity
                        assign stock_status = 'product--lowstock color-warning'
                      endif
                    -%}
                    <span class="product-inventory-tracking icon-dot-shadow {{ stock_status }} p-relative">
                      {%- if settings.product_information_low_stock_threshold > 0 and block.settings.show_stock_number -%}
                        {{ 'products.product.in_stock_html' | t: stock: product.selected_or_first_available_variant.inventory_quantity }}
                      {%- else -%}
                        {{ 'products.product.in_stock' | t }}
                      {%- endif -%}
                    </span>
                  {%- else -%}
                    {%- if product.selected_or_first_available_variant.inventory_policy != 'continue' and product.selected_or_first_available_variant.inventory_management != blank -%}
                      <span class="product-inventory-tracking icon-dot-shadow product--outstock p-relative color-error">{{ 'products.product.out_of_stock' | t }}</span>
                    {% else %}
                      <span class="product-inventory-tracking icon-dot-shadow product--instock color-success p-relative">
                      {{ 'products.product.in_stock' | t }}
                      </span>
                    {%- endif -%}
                  {%- endif -%}
                </div>
              </div>
            {%- when 'purchasing' -%}
              <div class="product__info ord-{{ forloop.index }} d-block" data-section-id="{{ section.id }}" data-is-enabled="{{ section.settings.show_sticky_purchasing_bar }}">
                <div class="js-sticky-product-info " {{ block.shopify_attributes }}>
                  <div class="product__form">
                    <sht-prd-frm class="sht-product__form d-grid gap-12" data-section="{{ section.id }}">
                      <div class="form--error js-product-form-error-wrapper" data-type="error-wrapper" role="alert" hidden>
                        <span class="d-flex color-error product-form-message">
                          {%- render 'icon-error', size: '22', class: 'mr-8 flex-1 fs-0' -%}
                          <span class="js-product-form-error-message subtext" data-type="error-message"></span>
                        </span>
                      </div>
                      {%- form 'product', product, id: product_form_id, class: 'form form__fields js-main-product-form', novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
                        <div class="form__field">
                          <input type="hidden" class="form__control" name="id" value="{{ product.selected_or_first_available_variant.id }}" disabled>
                        </div>
                        <div class="product-form__buttons d-flex top-xs fd-column">
                          {%-liquid
                          if block.settings.show_gift_card_recipient and product.gift_card?
                            render 'gift-card-recipient-form', product: product, form: form, section: section
                          endif
                         -%}
                          <div class="gap-16 d-flex w-100">
                            <div class="product__quantity {{ block.settings.show_quantity_selector | default: 'd-none' | replace: 'true', '' }}" id="productQuantity-{{ section.id }}" data-show="{{ block.settings.show_quantity_selector }}">
                              <label class="field__label visually-hidden" for="productQuantityInput-{{ section.id }}">{{ 'products.product.quantity.label' | t }}:</label>
                              <sht-qty-inp class="field_quantity d-inline-flex middle-xs center-xs">
                                <button class="no-js-hidden js-quantity-btn js-quantity-btn-minus btn btn-icon btn-large btn-plain" aria-label="{{ 'accessibility.minus' | t }}" name="minus" data-name="minus" type="button">{%- render 'icon-minus', size: 20 -%}</button>
                                <input
                                  class="js-quantity-input js-main-product-quantity-input qty-element input-number b-zero ta-center"
                                  type="number"
                                  name="quantity"
                                  aria-label="{{ 'products.product.quantity.input_label' | t: product: product.title | escape }}"
                                  id="productQuantityInput-{{ section.id }}"
                                  min="1"
                                  value="1"
                                  form="{{ product_form_id }}"
                                >
                                <button class="no-js-hidden js-quantity-btn js-quantity-btn-plus btn btn-icon btn-large btn-plain" aria-label="{{ 'accessibility.plus' | t }}" name="plus" data-name="plus" type="button">{%- render 'icon-plus', size: 20 -%}</button>
                              </sht-qty-inp>

                            </div>
                            <button type="submit" name="add" class="js-product-form-submit-btn product-btn-atc btn {% if block.settings.show_dynamic_checkout_buttons %} btn-secondary {% else %} btn-primary {% endif %} btn-large w-100"
                              {% if product.selected_or_first_available_variant.available == false %}
                                disabled
                                {% assign add_to_cart_btn_lbl =  'products.product.sold_out' | t %}
                              {% else %}
                                {% assign add_to_cart_btn_lbl =  'products.product.add_to_cart' | t %}
                              {% endif %}>
                              <span class="p-relative d-flex middle-xs center-xs ta-center">
                                <span class="js-product-form-spinner product-form-spinner p-absolute icon">{% render 'icon-spinner', class: 'spinner' %}</span>
                                <span class="js-product-form-submit-btn-text">
                                  {{ add_to_cart_btn_lbl }}
                                </span>
                              </span>
                            </button>
                          </div>
                          {% if block.settings.show_dynamic_checkout_buttons %}
                            <sht-load-dyn-co-btn>
                              <template>{{ form | payment_button }}</template>
                            </sht-load-dyn-co-btn>
                          {% endif %}
                        </div>
                      {%- endform -%}
                    </sht-prd-frm>
                  </div>
                </div>
              </div>
            {%- when 'highlights' -%}
              <div class="product__info ord-{{ forloop.index }} product__extra-info product__extra-info--{{ block.settings.display_style }} " {{ block.shopify_attributes }}>
                {% capture custom_icon_first %}
                  {%- liquid
                    assign icon_svg = ''
                    assign icon_source = block.settings.icon_source_1
                    if icon_source == 'theme_library'
                      render 'icon-loader', icon: block.settings.primary_options_icon, size: 36
                    elsif icon_source == 'custom_img'
                      assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                      assign custom_icon = block.settings.custom_img_1 | image_url: width: block.settings.custom_img_1.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.primary_heading, class: 'media of-cover h-100 w-100'
                      render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                    else
                      assign icon_svg = block.settings.html_code_1
                    endif
                    -%}
                    {%- if icon_svg != '' -%}
                      <div class="icon-custom--large h-100 w-100">{{- icon_svg -}}</div>
                    {%- endif -%}
                {% endcapture %}
                {% capture custom_icon_second %}
                  {%- liquid
                    assign icon_svg = ''
                    assign icon_source = block.settings.icon_source_2
                    if icon_source == 'theme_library'
                      render 'icon-loader', icon: block.settings.secondary_options_icon, size: 36
                    elsif icon_source == 'custom_img'
                      assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                      assign custom_icon = block.settings.custom_img_2 | image_url: width: block.settings.custom_img_2.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.secondary_heading, class: 'media of-cover h-100 w-100'
                      render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                    else
                      assign icon_svg = block.settings.html_code_2
                    endif
                    -%}
                    {%- if icon_svg != '' -%}
                      <div class="icon-custom--large h-100 w-100">{{- icon_svg -}}</div>
                    {%- endif -%}
                {% endcapture %}
                {% capture custom_icon_third %}
                {%- liquid
                  assign icon_svg = ''
                  assign icon_source = block.settings.icon_source_3
                  if icon_source == 'theme_library'
                    render 'icon-loader', icon: block.settings.tertiary_options_icon, size: 36
                  elsif icon_source == 'custom_img'
                    assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                    assign custom_icon = block.settings.custom_img_3 | image_url: width: block.settings.custom_img_3.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.tertiary_heading, class: 'media of-cover h-100 w-100'
                    render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                  else
                    assign icon_svg = block.settings.html_code_3
                  endif
                  -%}
                  {%- if icon_svg != '' -%}
                    <div class="icon-custom--large h-100 w-100">{{- icon_svg -}}</div>
                  {%- endif -%}
                {% endcapture %}
                {% capture custom_icon_fourth %}
                {%- liquid
                  assign icon_svg = ''
                  assign icon_source = block.settings.icon_source_4
                  if icon_source == 'theme_library'
                    render 'icon-loader', icon: block.settings.quaternary_options_icon, size: 36
                  elsif icon_source == 'custom_img'
                    assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                    assign custom_icon = block.settings.custom_img_4 | image_url: width: block.settings.custom_img_4.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.quaternary_heading, class: 'media of-cover h-100 w-100'
