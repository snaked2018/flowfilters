                            {{ custom_icon_third }}
                            <span class="h6 m-zero fg-1 ml-8 ta-left">{{ block.settings.tertiary_heading | escape }}</span>
                            <span class="accordion__toggle zi-1 d-flex">{{ accordion_icons }}</span>
                          </button>
                        </div>
                        <div class="js-accordion-content text-block accordion-content mb-24 pl-40 rte pac-tertiary--{{ forloop.index }}-{{- block.id -}}" id="accordion-tertiary-content-{{- block.id -}}" role="region" aria-describedby="accordion-tertiary-title-{{ block.id }}" tabindex="-1" style="max-height: 0;">
                          {{- block.settings.tertiary_content -}}
                        </div>
                      </div>
