{%- liquid
  assign title_placeholder = 'onboarding.product_title' | t
  assign section_id = section.id | md5 | truncate: 12, ''
  assign variant_label = 'color,colour,couleur,colore,boja,farbe,색,色,warna,dath,боја,lliw,liw,колір,värvi,värv,գույն,krāsa,màu sắc,màu,cor,cцвят,färg,farve,ngjyrë,kleur,väri,farge,renk,culoare,szín,цвет,kolor,barva,χρώμα,litur,spalva,farba' | split: ','
  assign place_holder = 'product-1' | placeholder_svg_tag: 'placeholder w-100 h-100'
  assign product_form_id = 'featuredProductForm-' | append: section.id
  assign is_app = false
  assign product = section.settings.product
  assign first_3d_model = product.media | where: 'media_type', 'model' | first
  assign enable_thumb_mobile = 'd-none d-flex-md'
  if section.settings.enable_thumb_mobile
    assign enable_thumb_mobile = 'd-flex top-xs'
  endif
  assign thumb_index = 0

  assign ratio_thubnails = section.settings.thumbnails_ratio
  assign object_fit = 'of-cover'
  if ratio_thubnails contains 'fit'
    assign ratio_thubnails = ratio_thubnails | replace: 'fit', ''
    assign object_fit = 'of-contain'
  endif
-%}
{%- if first_3d_model -%}
  <script type="application/json" id="ProductJSON-{{ product.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{%- endif -%}
<div class="ctnr" id="{{ section.id }}" data-ui-component="Featured Product Section" role="region" aria-labelledby="sectionHeading-{{ section.id | md5 | truncate: 12, "" }}">
  {% render 'section-header',
    subheading: section.settings.subheading,
    heading: section.settings.heading,
    description: section.settings.description,
    content_align: section.settings.content_alignment,
    section: section
  %}
  <div class="featured-product-details d-grid c-gap-md top-xs  gap-36 p-relative zi-1" style="--gtc-equal:1; --gtc-equal-md:2">
    <div class="product__media-wrapper js-animate anm anm-fadeInUpLarge" style="--duration: .7s; --delay: .15s">
      <div class="product__media-wrapper--single-with-thumbnails o-hidden">
        {% if section.settings.product != blank %}
          <sht-prd-slideshow class="product-slideshow d-flex fd-column gap-24 d-grid-md p-relative" data-hide-variant-image="true" data-section="{{ section.id }}">
            <div class="product-slideshow__thumbnail no-js-hidden {{ enable_thumb_mobile }}">
              <sht-vert-carousel class="carousel--vertical" data-section="{{ section.id }}">
                <div class="product-slideshow__thumbs {{ enable_thumb_mobile }} h-100 top-xs js-carousel-items vertical-carousel vertical-carousel__container fd-column-md p-zero m-zero oy-auto p-relative disable-scrollbars fw-nowrap scrollable scrollable-y scrl-bhvr-auto" role="list" aria-label="{{ 'accessibility.slider' | t }}">
                  {%- liquid
                    if product.selected_or_first_available_variant.featured_media != null
                      assign featured_media = product.selected_or_first_available_variant.featured_media
                      assign thumb_index = thumb_index | plus: 1
                      render 'product-slideshow-thumb', section: section, media: featured_media, product: product, place_holder: place_holder, thumb_index: thumb_index, ratio: ratio_thubnails, object_fit: object_fit
                    endif
                    for media in product.media
                      assign thumb_index = thumb_index | plus: 1
                      unless media.id == product.selected_or_first_available_variant.featured_media.id
                        render 'product-slideshow-thumb', section: section, media: media, product: product, place_holder: place_holder, thumb_index: thumb_index, ratio: ratio_thubnails, object_fit: object_fit
                      endunless
                    endfor
                  -%}
                </div>
                <div class="product-slideshow__pagination carousel-pagination carousel-controls--outside maw-100 center-xs no-js-hidden js-carousel-pagination d-flex">
                  <div class="carousel-btn carousel-btn-prev p-absolute">
                    <button type="button" data-step="1" class="btn btn-secondary btn-small btn-icon icon btn-prev js-carousel-prev-btn" disabled aria-label="{{ 'accessibility.previous_slide' | t }}" name="previous">
                      {% render 'icon-arrow-left' %}
                    </button>
                  </div>
                  <div class="carousel-btn carousel-btn-next p-absolute">
                    <button type="button" data-step="1" class="btn btn-secondary btn-small btn-icon icon btn-next p-absolute js-carousel-next-btn" disabled aria-label="{{ 'accessibility.next_slide' | t }}" name="next">
                      {% render 'icon-arrow-right' %}
                    </button>
                  </div>
                </div>
              </sht-vert-carousel>
            </div>
            <div class="product-slideshow__slider">
              {% render 'product-gallery-carousel', product: product, section: section, place_holder: place_holder, rand: section_id, hide_variant_image: true, is_first_image: false, open_zoom_modal: false, ratio: section.settings.media_ratio %}
            </div>
          </sht-prd-slideshow>
        {% else %}
          <div class="media" style="--ratio: 1/1">
            {{ place_holder }}
          </div>
        {% endif %}
      </div>
    </div>
    <div class="product__info-wrapper js-animate anm anm-fadeInUpLarge anm-featured-product" style="--duration: .7s;">
      <div id="productInfo-{{ section.id }}" class="featured-product-container product__info-container d-flex fd-column">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when '@app' -%}
              <div class="product__info">
                {% liquid
                  assign is_app = true
                  render block
                %}
              </div>
            {%- when 'title' -%}
              <div class="product__title product__info product__info--spacing d-flex middle-xs between-xs" {{ block.shopify_attributes }}>
                <h3 class="h3 m-zero">
                  <a href="{{ product.url | default: '#' }}" class="pe-no-link">
                    {{ product.title | escape | default: title_placeholder }}
                  </a>
                </h3>
                {%- if block.settings.enable_share -%}
                  <sht-share-prd-btn class="zi-2 p-relative" data-product-url="{{ product.selected_variant.url | default: product.url | prepend: request.origin }}">
                    <div class="js-web-share-api-container" hidden>
                      <button class="btn btn-large ls-none btn-icon icon btn btn-secondary js-web-share-api-btn" aria-label="{{ 'accessibility.social_share' | t }}">{% render 'icon-share' %}</button>
                    </div>
                    <sht-dtl-exposal>
                      <details class="js-none-web-share-api-container p-relative zi-2" id="productShareDetails-{{ block.id }}-{{ section.id }}">
                        <summary aria-controls="productShare-{{ block.id }}-{{ section.id }}" aria-expanded="false" class="btn btn-large ls-none btn-icon icon btn btn-secondary js-none-web-share-api-btn">
                          {% render 'icon-share', pros: 'aria-hidden="true" focusable="false"' %}
                        </summary>
                        <div id="productShare-{{ block.id }}-{{ section.id }}" class="p-absolute social-share-list">
                          {% render 'product-social-share', product: product %}
                        </div>
                      </details>
                    </sht-dtl-exposal>
                  </sht-share-prd-btn>
                {%- endif -%}
              </div>
            {%- when 'vendor' -%}
              <div class="product__vendor product__info product__info--spacing wb-break-word" {{ block.shopify_attributes }}>
                <div class="color-light subtext wb-break-word">
                  <span class="visually-hidden">{{ 'accessibility.vendor' | t }}</span>{{ product.vendor }}
                </div>
              </div>
            {%- when 'rating' -%}
              <div class="product__info product__rating product__info--spacing" {{ block.shopify_attributes }}>
                {% render 'product-rating', product: product %}
              </div>
            {%- when 'price' -%}
              <div class="product__info product__price-and-extrainfo product__prices d-grid gap-12" {{ block.shopify_attributes }}>
                <div class="d-flex fw-wrap gap-8" id="featuredProductPrice-{{ section.id }}" role="status">
                  {% render 'product-price', product: product, target: product.selected_or_first_available_variant, price_class: 'price--large', price_style: 'mr-8' %}
                    {% if product.selected_or_first_available_variant.compare_at_price > product.selected_or_first_available_variant.price and block.settings.show_sale_badge %}
                      {%- liquid
                        assign discount_cost = product.selected_or_first_available_variant.compare_at_price | minus: product.selected_or_first_available_variant.price
                        assign discount_percent = discount_cost | times: 100.0 | divided_by: product.selected_or_first_available_variant.compare_at_price | round: 2
                      -%}
                      <div class="product__badge">
                        <span class="badge badge--sale subtext d-inline-block p-relative fw-700">
                          <span class="p-relative zi-1 tt-up">
                            {{- 'products.product.on_sale' | t }}
                              {{ discount_percent }}%
                            <span class="d-none-important">
                            {% if settings.currency_code_enabled %}
                              {{- discount_cost | money_with_currency -}}
                            {% else %}
                              {{- discount_cost | money -}}
                            {% endif %}

                              ({{ discount_percent }}%)
                            </span>
                          </span>
                        </span>
                      </div>
                    {% endif %}
                </div>
                {% if block.settings.show_tax_and_shipping_policy %}
                  <div class="color-light hide-empty subtext text-block">
                    {%- if cart.taxes_included -%}
                      {{- 'products.product.include_taxes' | t -}}
                    {%- endif -%}
                    {%- if shop.shipping_policy.body != blank -%}
                      {{- 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url -}}
                    {%- endif -%}
                  </div>
                {% endif %}
              </div>
            {%- when 'SKU' -%}
              <div class="product__info product__sku product__info--spacing gap-24 d-flex fd-column" id="featuredSku-{{ section.id }}" {{ block.shopify_attributes }}>
                {%- if product.selected_or_first_available_variant.sku != blank -%}
                  <span class="variant-sku color-light">SKU: {{ product.selected_or_first_available_variant.sku }}</span>
                {%- endif -%}
              </div>
              {%- when 'description' -%}
                <div class="product__info product__description wb-break-word  rte" {{ block.shopify_attributes }}>
                  <span class="d-block mb-16 h5">{{ 'products.product.description' | t }}</span>
                  {% if block.settings.enable_more_btn %}
                    <sht-read-more>
                      <div class="expandable-content p-relative o-hidden js-expandable-content" data-expanded="false">
                        <div class="rte">
                          {{ product.description }}
                        </div>
                      </div>
                  <div class="expandable-content__btn w-100 ta-left p-relative">
                    <button type="button" class="btn btn-link js-expandable-content-btn" data-view-more="{{ 'products.read_more.view_more' | t }}" data-view-less="{{ 'products.read_more.view_less' | t }}">
                      {{ 'products.read_more.view_more' | t }}
                    </button>
                  </div>
                    </sht-read-more>
                  {% else %}
                    <div class="rte">
                      {{ product.description }}
                    </div>
                  {% endif %}

                </div>
            {%- when 'variants' -%}
              <div class="product__info product__variants gap-24 d-flex fd-column" {{ block.shopify_attributes }}>
                {%- unless product.has_only_default_variant -%}
                  <div class="" id="featuredVariantPicker-{{ section.id }}">
                    <sht-featured-variant-radios class="product__variants--radios d-flex fd-column gap-24 no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                      {%- for option in product.options_with_values -%}
                        {% assign index = forloop.index | minus: 1 %}
                        <fieldset class="form__fields gap-12 product-option js-featured-variant-radio-container d-flex fw-wrap p-zero m-zero b-zero">
                          <legend class="p-zero m-zero product-option__name">
                            <span class="mr-8 fw-700">{{ option.name }}</span>
                            <span class="js-variant-radio-option-name hide-empty" data-option-name="{{ option.name }}">
                              {{ product.selected_or_first_available_variant.options[index] }}
                            </span>
                          </legend>
                          {%- assign variant_name = option.name | downcase -%}
                          {%- if variant_label contains variant_name and settings.enable_variant_swatches -%}
                            {% render 'featured-product-color-swatch', product: product, option: option, section: section, product_form_id: product_form_id, index: index %}
                          {%- else -%}
                            {%- for value in option.values -%}
                              <div class="form__field js-variant-option-value product-option__value {% if product.selected_or_first_available_variant.available == false and option.selected_value == value %}product-option--sold-out{% endif %}" data-value="{{ value | escape }}-{{ index }}" data-option-index="{{ index }}">
                                <input type="radio" class="form__control input--variant js-variant-item js-featured-variant-radio-item visually-hidden" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}" name="{{ option.name }}" value="{{ value | escape }}" data-variant-value="{{ value | escape }}-{{ index }}" form="{{ product_form_id }}"{% if option.selected_value == value %} checked{% endif %}>
                                <label class="form__label product-option__label d-block" for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}">{{ value }}</label>
                              </div>
                            {%- endfor -%}
                          {%- endif -%}
                        </fieldset>
                      {%- endfor -%}
                      <script type="application/json">
                        {{ product.variants | json }}
                      </script>
                    </sht-featured-variant-radios>
                  </div>
                  <noscript>
                    <div class="form__fields {% if product.has_only_default_variant %} hidden{% endif %}">
                      <div class="form__field">
                        <label class="form__label" for="Variants-{{ section.id }}-{{ forloop.index0 }}">{{ 'products.product.product_variants' | t }}</label>
                        <select name="id" id="Variants-{{ section.id }}-{{ forloop.index0 }}" class="form__control w-100 h-100 field__text field--focus field--hover" form="{{ product_form_id }}">
                          {%- for variant in product.variants -%}
                            <option {% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %} {% if variant.available == false %}disabled{% endif %} value="{{ variant.id }}">{{ variant.title }}{%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %} - {{ variant.price | money | strip_html }}</option>
                          {%- endfor -%}
                        </select>
                      </div>
                    </div>
                  </noscript>
                {%- endunless -%}
                <div class="hide-empty fw-700" id="featuredInventoryTracking-{{ section.id }}">
                  {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                    {%- liquid
                      assign stock_status = 'product--instock color-success'
                      if settings.product_information_low_stock_threshold > 0 and settings.product_information_low_stock_threshold >= product.selected_or_first_available_variant.inventory_quantity
                        assign stock_status = 'product--lowstock color-warning'
                      endif
                    -%}
                    <span class="product-inventory-tracking icon-dot-shadow {{ stock_status }} p-relative">
                      {%- if settings.product_information_low_stock_threshold > 0 and block.settings.show_stock_number -%}
                        {{ 'products.product.in_stock_html' | t: stock: product.selected_or_first_available_variant.inventory_quantity }}
                      {%- else -%}
                        {{ 'products.product.in_stock' | t }}
                      {%- endif -%}
                    </span>
                  {%- else -%}
                    {%- if product.selected_or_first_available_variant.inventory_policy != 'continue' and product.selected_or_first_available_variant.inventory_management != blank -%}
                      <span class="product-inventory-tracking icon-dot-shadow product--outstock p-relative color-error">{{ 'products.product.out_of_stock' | t }}</span>
                    {% else %}
                      <span class="product-inventory-tracking icon-dot-shadow product--instock color-success p-relative">
                      {{ 'products.product.in_stock' | t }}
                      </span>
                    {%- endif -%}
                  {%- endif -%}
                </div>
              </div>
            {%- when 'purchasing' -%}
              <div class="product__info" {{ block.shopify_attributes }}>
                <div class="product__form">
                  <sht-featured-prd-frm data-section="{{ section.id }}">
                    <div class="js-featured-product-form-error-wrapper mb-16" data-type="error-wrapper" role="alert" hidden>
                      <span class="d-flex middle-xs">
                        <span class="icon--error icon icon--small mr-8 fs-0">{%- render 'icon-error', size: 16 -%}</span>
                        <span class="js-featured-product-form-error-message subtext" data-type="error-message"></span>
                      </span>
                    </div>
                    {%- if product != blank -%}
                      {%- form 'product', product, id: product_form_id, class: 'form form__fields', novalidate: 'novalidate' -%}
                        <div class="form__field">
                          <input type="hidden" class="form__control" name="id" value="{{ product.selected_or_first_available_variant.id }}" disabled>
                        </div>
                        <div class="product-form__buttons d-flex top-xs fd-column">
                          <div class="gap-16 d-flex w-100">
                            <div class="product__quantity {{ block.settings.show_quantity_selector | default: 'd-none' | replace: 'true', '' }} " id="featuredProductQuantity-{{ section.id }}" data-show="{{ block.settings.show_quantity_selector }}">
                              <label class="field__label visually-hidden" for="featuredProductQuantityInput-{{ block.id }}-{{ section.id }}">{{ 'products.product.quantity.label' | t }}:</label>
                              <sht-qty-inp class="field_quantity d-inline-flex middle-xs center-xs">
                                <button class="no-js-hidden js-quantity-btn js-quantity-btn-minus btn btn-icon btn-large btn-plain" aria-label="{{ 'accessibility.minus' | t }}" name="minus" data-name="minus" type="button">
                                  {%- render 'icon-minus', size: 20 -%}
                                </button>
                                <input class="js-quantity-input qty-element input-number btn-plain b-zero ta-center" type="number" name="quantity" aria-label="{{ 'products.product.quantity.input_label' | t: product: product.title | escape }}" id="featuredProductQuantityInput-{{ block.id }}-{{ section.id }}" min="1" value="1" form="{{ product_form_id }}">
                                <button class="no-js-hidden js-quantity-btn js-quantity-btn-plus btn btn-icon btn-large btn-plain" aria-label="{{ 'accessibility.plus' | t }}" name="plus" data-name="plus" type="button">
                                  {%- render 'icon-plus', size: 20 -%}
                                </button>
                              </sht-qty-inp>
                            </div>
                            <button type="submit" name="add" class="js-featured-product-form-submit-btn product-btn-atc btn btn-secondary btn-large w-100"
                              {% if product.selected_or_first_available_variant.available == false %}
                                disabled
                                {% assign add_to_cart_btn_lbl =  'products.product.sold_out' | t %}
                              {% else %}
                                {% assign add_to_cart_btn_lbl =  'products.product.add_to_cart' | t %}
                              {% endif %}>
                              <span class="p-relative d-flex middle-xs center-xs ta-center">
                                <span class="hidden js-featured-product-form-spinner">
                                  {% render 'icon-spinner' %}
                                </span>
                                <span class="js-featured-product-form-submit-btn-text">
                                  {{ add_to_cart_btn_lbl }}
                                </span>
                              </span>
                            </button>
                          </div>
                          {% if block.settings.show_dynamic_checkout_buttons %}
                            <sht-load-dyn-co-btn class="w-100 d-block">
                              <template>
                                {{ form | payment_button }}
                              </template>
                            </sht-load-dyn-co-btn>
                          {% endif %}
                        </div>
                      {%- endform -%}
                    {%- else -%}
                      <button type="submit" name="add" class=" product-btn-atc btn btn-secondary btn-large mb-16 w-100" disabled>
                        {{ 'products.product.sold_out' | t }}
                      </button>
                    {%- endif -%}
                  </sht-featured-prd-frm>
                </div>
              </div>
            {%- when 'view_details_button' -%}
              <div class="product__info d-flex center-xs" {{ block.shopify_attributes }}>
                <a href="{{ product.url | default : '#' }}" class="btn btn-{{- block.settings.button_style }} btn-large">{{ 'products.product.view_details' | t }}</a>
              </div>
            {%- when 'highlights' -%}
              <div class="product__info product__extra-info product__extra-info--{{- block.settings.display_style }}" {{ block.shopify_attributes }}>
                {% capture custom_icon_first %}
                  {%- liquid
                    assign icon_svg = ''
                    assign icon_source = block.settings.icon_source_1
                    if icon_source == 'theme_library'
                      render 'icon-loader', icon: block.settings.primary_options_icon, size: 36
                    elsif icon_source == 'custom_img'
                      assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                      assign custom_icon = block.settings.custom_img_1 | image_url: width: block.settings.custom_img_1.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.primary_heading, class: 'media of-cover h-100 w-100'
                      render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                    else
                      assign icon_svg = block.settings.html_code_1
                    endif
                    -%}
                    {%- if icon_svg != '' -%}
                      <div class="icon-custom--large h-100 w-100">
                        {{- icon_svg -}}
                      </div>
                    {%- endif -%}
                {% endcapture %}
                {% capture custom_icon_second %}
                  {%- liquid
                    assign icon_svg = ''
                    assign icon_source = block.settings.icon_source_2
                    if icon_source == 'theme_library'
                      render 'icon-loader', icon: block.settings.secondary_options_icon, size: 36
                    elsif icon_source == 'custom_img'
                      assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                      assign custom_icon = block.settings.custom_img_2 | image_url: width: block.settings.custom_img_2.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.secondary_heading, class: 'media of-cover h-100 w-100'
                      render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                    else
                      assign icon_svg = block.settings.html_code_2
                    endif
                    -%}
                    {%- if icon_svg != '' -%}
                      <div class="icon-custom--large h-100 w-100">
                        {{- icon_svg -}}
                      </div>
                    {%- endif -%}
                {% endcapture %}
                {% capture custom_icon_third %}
                {%- liquid
                  assign icon_svg = ''
                  assign icon_source = block.settings.icon_source_3
                  if icon_source == 'theme_library'
                    render 'icon-loader', icon: block.settings.tertiary_options_icon, size: 36
                  elsif icon_source == 'custom_img'
                    assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                    assign custom_icon = block.settings.custom_img_3 | image_url: width: block.settings.custom_img_3.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.tertiary_heading, class: 'media of-cover h-100 w-100'
                    render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                  else
                    assign icon_svg = block.settings.html_code_3
                  endif
                  -%}
                  {%- if icon_svg != '' -%}
                    <div class="icon-custom--large h-100 w-100">
                      {{- icon_svg -}}
                    </div>
                  {%- endif -%}
                {% endcapture %}
                {% capture custom_icon_fourth %}
                {%- liquid
                  assign icon_svg = ''
                  assign icon_source = block.settings.icon_source_4
                  if icon_source == 'theme_library'
                    render 'icon-loader', icon: block.settings.quaternary_options_icon, size: 36
                  elsif icon_source == 'custom_img'
                    assign custom_icon = 'image' | placeholder_svg_tag: 'placeholder media of-cover'
                    assign custom_icon = block.settings.custom_img_4 | image_url: width: block.settings.custom_img_4.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: block.settings.quaternary_heading, class: 'media of-cover h-100 w-100'
                    render 'image', image_item: custom_icon, image_wrap_classes: "icon-custom--large d-block", image_classes: "h-100 w-100"
                  else
                    assign icon_svg = block.settings.html_code_4
                  endif
                  -%}
                  {%- if icon_svg != '' -%}
                    <div class="icon-custom--large h-100 w-100">
                      {{- icon_svg -}}
                    </div>
                  {%- endif -%}
                {% endcapture %}
              {% if block.settings.display_style == 'collapsibles' %}
                  {% capture accordion_icons %}
                    <span class="zi-1 btn btn-small btn-icon">
                      {% render 'icon-chevron-down', size: '24' class: 'icon-chevron-down p-absolute' %}
                    </span>
                  {% endcapture %}
