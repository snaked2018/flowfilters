                        <div class="accordion__header">
                          <button class="btn-plain w-100 js-accordion-trigger d-flex middle-xs p-relative pt-16 pb-16 p-zero c-pointer" id="accordion-secondary-title-{{- block.id -}}" aria-expanded="false" aria-controls="accordion-secondary-content-{{- block.id -}}">
                            {{ custom_icon_second }}
                            <span class="h6 m-zero fg-1 ml-8 ta-left">{{ block.settings.secondary_heading | escape }}</span>
