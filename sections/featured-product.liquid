                          {{- block.settings.quaternary_content -}}
                        </div>
                      </div>
                    {% endif %}
                  </sht-accordion>
                {% else %}
                  <div class="rw r-gap-20 r-gap-md-36">
                    <div class="clm" style="--w-xs: 12; --w-sm: 6;">
                      {%- if block.settings.primary_heading != blank -%}
