                            {{ custom_icon_first }}
                            <span class="h6 m-zero fg-1 ml-8 ta-left">{{ block.settings.primary_heading | escape }}</span>
                            <span class="accordion__toggle zi-1 d-flex">{{ accordion_icons }}</span>
                          </button>
                        </div>
                        <div class="js-accordion-content accordion-content mb-24 pl-40 rte pac-primary--{{ forloop.index }}-{{ block.id }}" id="accordion-primary-content-{{- block.id -}}" role="region" aria-describedby="accordion-primary-title-{{ block.id }}" tabindex="-1" style="max-height: 0;">
